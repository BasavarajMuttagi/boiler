{
  "name": "bo1ler",
  "version": "0.0.1",
  "description": "Component Library",
  "main": "index.js",
  "scripts": {
    "commitlint": "commitlint -c -e",
    "start": "start-storybook -p 1337",
    "build": "NODE_ENV=production webpack --config webpack.prod.js",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "format": "npm run format:eslint && npm run format:prettier",
    "lint:eslint": "eslint --ext .ts . --ignore-path .gitignore",
    "format:eslint": "eslint --ext .ts . --fix --ignore-path .gitignore",
    "lint:prettier": "prettier \"**/*.ts\" --check --ignore-path .gitignore",
    "format:prettier": "prettier \"**/*.ts\" --write --ignore-path .gitignore",
    "build-storybook": "build-storybook",
    "doc:install": "deven-documentation-skeleton install",
    "doc:check": "deven-documentation-skeleton check",
    "doc:update": "deven-documentation-skeleton update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orivaa/BO1LER.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/orivaa/BO1LER/issues"
  },
  "homepage": "https://github.com/orivaa/BO1LER#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-decorators": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.0",
    "@types/react": "^18.0.27",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "deven-documentation-skeleton": "^1.5.0",
    "eslint": "^8.32.0",
    "eslint-plugin-lit": "^1.8.2",
    "eslint-plugin-lit-a11y": "^1.1.0-next.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.10",
    "eslint-plugin-wc": "^1.4.0",
    "eslint-webpack-plugin": "^3.2.0",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "prettier": "2.8.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "release-please": "^15.1.1",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "lit": "^2.6.1"
  }
}
